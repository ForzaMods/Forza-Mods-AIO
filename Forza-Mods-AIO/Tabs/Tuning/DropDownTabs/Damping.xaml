                                       BorderThickness="0" 
                                       TrackMouseWheelWhenMouseOver="True" 
                                       TextAlignment="Left" 
                                       ParsingNumberStyle="Float" 
                                       NumericInputMode="Decimal" 
                                       Height="25" 
                                       Width="370" 
                                       HorizontalAlignment="Right" 
                                       VerticalAlignment="Top" 
                                       Margin="7.5" 
                                       ValueChanged="ValueChanged"/>
                    
                </Grid>    
            </Border>
            
            <Border Style="{StaticResource CommonBorderStyle}">
                <Grid>
                    
                    <Label Content="Front Bump Stiffness Min" 
                           HorizontalAlignment="Left" 
                           VerticalAlignment="Top" 
                           Margin="7.5"/>
                    
                    <mah:NumericUpDown x:Name="FrontBumpStiffnessMinBox" 
                                       Background="#FF2E3440" 
                                       BorderThickness="0" 
                                       TrackMouseWheelWhenMouseOver="True" 
                                       TextAlignment="Left" 
                                       ParsingNumberStyle="Float" 
                                       NumericInputMode="Decimal" 
                                       Height="25" 
                                       Width="120" 
                                       HorizontalAlignment="Left" 
                                       VerticalAlignment="Top" 
                                       Margin="152.5, 7.5" 
                                       ValueChanged="ValueChanged"/>
                    
                    <Label Content="Front Bump Stiffness Max" 
                           HorizontalAlignment="Right" 
                           VerticalAlignment="Top" 
                           Margin="0,7.5,130,0"/>

                    <mah:NumericUpDown Background="#FF2E3440" 
                                       BorderThickness="0" 
                                       TrackMouseWheelWhenMouseOver="True" 
                                       x:Name="FrontBumpStiffnessMaxBox" 
                                       TextAlignment="Left" 
                                       ParsingNumberStyle="Float" 
                                       NumericInputMode="Decimal" 
                                       Height="25" 
                                       Width="120" 
                                       HorizontalAlignment="Right" 
                                       VerticalAlignment="Top" 
                                       Margin="7.5" 
                                       ValueChanged="ValueChanged"/>

                    
                </Grid>    
            </Border>
            
            <Border Style="{StaticResource CommonBorderStyle}">
                <Grid>
                    
                    <Label Content="Rear Bump Stiffness Min" 
                           HorizontalAlignment="Left" 
                           VerticalAlignment="Top" 
                           Margin="7.5"/>
                    
                    <mah:NumericUpDown x:Name="RearBumpStiffnessMinBox" 
                                       Background="#FF2E3440" 
                                       BorderThickness="0" 
                                       TrackMouseWheelWhenMouseOver="True" 
                                       TextAlignment="Left" 
                                       ParsingNumberStyle="Float" 
                                       NumericInputMode="Decimal" 
                                       Height="25" 
