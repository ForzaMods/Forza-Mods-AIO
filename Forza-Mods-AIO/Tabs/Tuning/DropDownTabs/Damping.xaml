                    <mah:NumericUpDown Background="#FF2E3440" 
                                       BorderThickness="0" 
                                       TrackMouseWheelWhenMouseOver="True" 
                                       x:Name="RearBumpStiffnessMaxBox" 
                                       TextAlignment="Left" 
                                       ParsingNumberStyle="Float" 
                                       NumericInputMode="Decimal" 
                                       Height="25" 
                                       Width="120" 
                                       HorizontalAlignment="Right" 
                                       VerticalAlignment="Top" 
                                       Margin="7.5" 
                                       ValueChanged="ValueChanged"/>

                    
                </Grid>    
            </Border>
        </StackPanel>
        
    </Grid>
</Page>
