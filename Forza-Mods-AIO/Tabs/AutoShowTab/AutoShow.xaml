        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy4" Height="32" Margin="189,317,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars_Copy4" Content="Fix Thumbnails" HorizontalAlignment="Left" Margin="167,286,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy5" Height="32" Margin="51,317,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars_Copy5" Content="Unlock Hidden Presets" HorizontalAlignment="Left" Margin="8,286,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy6" Height="32" Margin="188,240,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars_Copy6" Content="Clear Garage" HorizontalAlignment="Left" Margin="171,207,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy7" Height="32" Margin="375,287,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
