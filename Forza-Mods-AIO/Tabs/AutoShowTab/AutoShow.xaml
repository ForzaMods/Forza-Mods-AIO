        <Button x:Name="ScanButton" Content="Scan" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" BorderBrush="#FF2E3440" Focusable="False" FontFamily="Manrope" Height="25" Click="ScanButton_Click"/>
        <mah:ToggleSwitch x:Name="ToggleAllCars" Height="32" Margin="457,41,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleAllCars_Toggled"/>
        <Label x:Name="LabelAllCars" Content="All Cars" HorizontalAlignment="Left" Margin="457,10,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleRareCars" Height="32" Margin="538,41,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleRareCars_Toggled"/>
        <Label x:Name="LabelRareCars" Content="Rare Cars" HorizontalAlignment="Left" Margin="530,10,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars" Height="32" Margin="50,164,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars" Content="Free Cars" HorizontalAlignment="Left" Margin="43,133,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy" Height="32" Margin="188,84,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
