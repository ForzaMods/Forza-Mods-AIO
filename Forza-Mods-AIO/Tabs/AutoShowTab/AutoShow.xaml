        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy8" Height="32" Margin="511,287,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars_Copy8" Content="Free Visual Upgrades" HorizontalAlignment="Left" Margin="472,256,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy9" Height="32" Margin="363,364,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars_Copy9" Content="Quick Add All Cars" HorizontalAlignment="Left" Margin="331,333,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy10" Height="32" Margin="511,364,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
