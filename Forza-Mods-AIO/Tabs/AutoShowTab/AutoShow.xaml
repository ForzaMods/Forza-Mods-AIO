        <Label x:Name="LabelFreeCars_Copy1" Content="Rmove Any Car" HorizontalAlignment="Left" Margin="27,58,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy2" Height="32" Margin="320,89,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
        <Label x:Name="LabelFreeCars_Copy2" Content="Unlock Hidden Decals" HorizontalAlignment="Left" Margin="279,58,0,0" VerticalAlignment="Top"/>
        <mah:ToggleSwitch x:Name="ToggleFreeCars_Copy3" Height="32" Margin="51,238,0,0" VerticalAlignment="Top" Width="44" MinHeight="0" MinWidth="0" Content="" Toggled="ToggleFreeCars_Toggled"/>
